<ul>
  <h1>Searched results</h1>
  <% travel.forEach((travel) => { %>
    <li>
      <%= travel.title%>
    </li>
    <li>
      <%= travel.place%>
    </li>
    <img src="<%= travel.imageUrl%>" style="width:125px; heigth:auto;">
    <li style="list-style-type: none;">
      <form action="/travels/<%= travel._id %> ">
        <button type="submit" class="btn btn-primary"> + info</button>
      </form>
    </li>
    <% }) %>
</ul>
