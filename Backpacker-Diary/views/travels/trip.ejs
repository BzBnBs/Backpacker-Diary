<h1><%= travel.place %></h1>
<h3>" <%= travel.title %> "</h3>
<img src="<%= travel.imageUrl %>">
<p><%= travel.date %></p>
<p><%= travel.description %></p>

<form action="/travels/<%= travel._id %>/edit">
    <button type="submit">Edit Trip</button>
</form>

<form action="/travels/<%= travel._id %>/delete" method="POST">
    <button type="submit" class="btn btn-primary">Delete Trip</button>
</form>

